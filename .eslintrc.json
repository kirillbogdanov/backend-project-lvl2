{
    "plugins": [
        "jest"
    ],
    "env": {
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off"
    }
}
